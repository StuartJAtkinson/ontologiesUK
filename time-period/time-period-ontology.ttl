	rdfs:isDefinedBy <http://parliament.uk/ontologies/time-period/> .

pt:publicBillOfficeNonSittingTablingDayInTerm rdf:type owl:ObjectProperty ;
	rdfs:label "Public Bill Office non-sitting tabling day in term"@en ;
	rdfs:comment "Relates a Public Bill Office non-sitting tabling day to the term in which that Public Bill Office non-sitting tabling day takes place."@en ;
	rdfs:domain pt:PublicBillOfficeNonSittingTablingDay ;
	rdfs:range pt:Term ;
	rdfs:isDefinedBy <http://parliament.uk/ontologies/time-period/> .

pt:publicBillOfficeNonSittingTablingDayInRecess rdf:type owl:ObjectProperty ;
	rdfs:label "Public Bill Office non-sitting tabling day in recess"@en ;
	rdfs:comment "Relates a Public Bill Office non-sitting tabling day to the recess in which that Public Bill Office non-sitting tabling day takes place."@en ;
	rdfs:domain pt:PublicBillOfficeNonSittingTablingDay ;
