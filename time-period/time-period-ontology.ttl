	rdfs:isDefinedBy <http://parliament.uk/ontologies/time-period/> .
	
	
	
#######  Data properties ##### 

pt:ordinality rdf:type owl:DatatypeProperty ;
	rdfs:label "Ordinality"@en ;
	rdfs:comment "Relates a temporal thing to its ordinality within a wider temporal thing."@en ;
	rdfs:domain pt:TemporalThing ;
	rdfs:range xsd:integer ;
	rdfs:isDefinedBy <http://parliament.uk/ontologies/time-period/> .

pt:proposedStartDate rdf:type owl:DatatypeProperty ;
	rdfs:label "Proposed start date"@en ;
	rdfs:comment "Relates a temporal thing to its proposed start date."@en ;
	rdfs:domain pt:TemporalThing ;
	rdfs:range xsd:date ;
	rdfs:isDefinedBy <http://parliament.uk/ontologies/time-period/> .
	
pt:proposedEndDate rdf:type owl:DatatypeProperty ;
	rdfs:label "Proposed end date"@en ;
	rdfs:comment "Relates a temporal thing to its proposed end date."@en ;
	rdfs:domain pt:TemporalThing ;
	rdfs:range xsd:date ;
	rdfs:isDefinedBy <http://parliament.uk/ontologies/time-period/> .