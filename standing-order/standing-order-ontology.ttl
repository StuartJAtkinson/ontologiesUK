pso:issuedBy rdf:type owl:ObjectProperty ;
	rdfs:label "Issued by"@en ;
	rdfs:comment ""@en ;
	rdfs:domain pso:RevisionSet ;
	rdfs:range pso:House ;
	rdfs:isDefinedBy <http://parliament.uk/ontologies/standing-order/> .
	
